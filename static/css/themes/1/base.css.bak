/* THEME SPECIFIC */

/*BODY, GENERAL */
body {
    background-color: rgb(255, 255, 255);
    background-image: -webkit-gradient(linear, left top, left bottom, color-stop(0, rgb(245,245,245)), color-stop(1, rgb(255,255,255)));
    background-image: -moz-linear-gradient(top, rgb(245,245,245), rgb(255,255,255));
    filter:progid:DXImageTransform.Microsoft.Gradient(GradientType =3, startColorstr=rgb(245,245,245),endColorstr=rgb(255,255,255));        
}
#wrapper, wrapper {
    color: rgb(5, 0, 0);
    background-color: transparent;
}

/* ELEMENTS */

input, textarea, select, input.gsc-input {
    background-color: rgb(5,2,0);
    width:100%;
    color: rgb(255,240,230);
    outline: 1px solid rgb(165,165,165);
}

content input, content textarea, content select,
#content input, #content textarea, #content select {
    border-left: 1px solid rgb(245,245,245);
}

a:link {
    color: rgb(5, 0, 0);
    border-bottom:0.0008em solid rgb(90,90,90);
}
a:visited {
    color: rgb(15, 10, 10);
    border-bottom:0.0008em solid rgb(95,95,95);
}
a:hover, a:active {
    text-style: italic;
}

/* CUSTOM ELEMENTS */
#cli_web #_cmd {
    color: rgb(2,1,0);
    outline: 1px solid rgb(245,245,245);
}

iframe[name='photo_frame'] {
    background-color: rgb(0, 0, 0);
    background-image: -webkit-gradient(linear, left top, left bottom, color-stop(0, rgb(75,75,75)), color-stop(1, rgb(0,0,0)));
    background-image: -moz-linear-gradient(top, rgb(75,75,75), rgb(0,0,0));
    filter:progid:DXImageTransform.Microsoft.Gradient(GradientType =3, startColorstr=rgb(75,75,75),endColorstr=rgb(0,0,0));        
}
